<launch>
  <rosparam param="int">1</rosparam>
  <rosparam param="float">0.05</rosparam>
  <rosparam param="string">"hello"</rosparam>
  <rosparam param="int_array">[1, 2, 3]</rosparam>
  <rosparam param="float_array">[.05, .5, 5.]</rosparam>
  <rosparam param="string_array">["hello", "world", "."]</rosparam>
  <rosparam param="mixed_array">[1, "world", 3]</rosparam>

  <node pkg="rosserial_server" type="socket_node" name="rosserial_server">
    <param name="port" value="11413" />
  </node>
  <node pkg="rosserial_python" type="message_info_service.py" name="rosserial_message_info" />

  <test test-name="rosserial_server_socket_test_getparam" pkg="rosserial_test"
        type="rosserial_test_get_param" time-limit="10.0">
    <param name="mode" value="socket" />
    <param name="tcp_port" value="11413" />
  </test>
</launch>
